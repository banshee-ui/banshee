{
  "name": "banshee-ui",
  "description": "A mostly renderless Vue UI component and utility library.",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "Dillon Chanis",
    "email": "dillonchanis92@gmail.com"
  },
  "repository": "banshee-ui/banshee",
  "main": "dist/BansheeUI.umd.js",
  "module": "dist/BansheeUI.common.js",
  "unpkg": "dist/BansheeUI.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "e2e:open": "vue-cli-service e2e:open",
    "package": "vue-cli-service build --target lib --name BansheeUI src/index.js"
  },
  "dependencies": {
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.9",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-beta.9",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.9",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.9",
    "@vue/cli-service": "^3.0.0-beta.9",
    "@vue/eslint-config-standard": "^3.0.0-beta.9",
    "@vue/test-utils": "^1.0.0-beta.10",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "vue-template-compiler": "^2.5.13"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "vuejs",
    "banshee",
    "banshee-ui",
    "component",
    "components",
    "vue-components",
    "renderless",
    "javascript"
  ]
}
